{
  "name": "pydeodoriser",
  "description": "Python language extension for code smell",
  "author": "Team 27 | COMPSCI 399 | Semester One | University of Auckland",
  "repository": "https://github.com/uoa-compsci399-s1-2022/project-17/",
  "license": "Apache-2.0",
  "version": "0.11.3",
  "publisher": "openlawlibrary",
  "engines": {
    "vscode": "^1.62.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:python"
  ],
  "contributes": {
    "commands": [],
    "configuration": {
      "type": "object",
      "title": "pyDeodoriser",
      "properties": {
        "pyDeodoriser.substructures": {
          "type": "object",
          "description": "Toggle the detection of different patterns.",
          "properties": {
            "UnnecessaryElif": {
              "type": "boolean"
            },
            "IfElseReturnBool": {
              "type": "boolean"
            },
            "IfReturnBool": {
              "type": "boolean"
            },
            "IfElseAssignReturn": {
              "type": "boolean"
            },
            "IfElseAssignBoolReturn": {
              "type": "boolean"
            },
            "IfElseAssignBool": {
              "type": "boolean"
            },
            "EmptyIfBody": {
              "type": "boolean"
            },
            "EmptyElseBody": {
              "type": "boolean"
            },
            "NestedIf": {
              "type": "boolean"
            },
            "UnnecessaryElse": {
              "type": "boolean"
            },
            "DuplicateIfElseStatement": {
              "type": "boolean"
            },
            "SeveralDuplicateIfElseStatements": {
              "type": "boolean"
            },
            "DuplicateIfElseBody": {
              "type": "boolean"
            },
            "AugmentableAssignment": {
              "type": "boolean"
            },
            "DuplicateExpression": {
              "type": "boolean"
            },
            "MissedAbsoluteValue": {
              "type": "boolean"
            },
            "RepeatedAddition": {
              "type": "boolean"
            },
            "RepeatedMultiplication": {
              "type": "boolean"
            },
            "RedundantArithmetic": {
              "type": "boolean"
            },
            "RedundantNot": {
              "type": "boolean"
            },
            "RedundantComparison": {
              "type": "boolean"
            },
            "MergeableEqual": {
              "type": "boolean"
            }
          },
          "additionalProperties": false,
          "default": {
            "UnnecessaryElif": true,
            "IfElseReturnBool": true,
            "IfReturnBool": true,
            "IfElseAssignReturn": true,
            "IfElseAssignBoolReturn": true,
            "IfElseAssignBool": true,
            "EmptyIfBody": true,
            "EmptyElseBody": true,
            "NestedIf": true,
            "UnnecessaryElse": true,
            "DuplicateIfElseStatement": true,
            "SeveralDuplicateIfElseStatements": true,
            "DuplicateIfElseBody": true,
            "AugmentableAssignment": true,
            "DuplicateExpression": true,
            "MissedAbsoluteValue": true,
            "RepeatedAddition": true,
            "RepeatedMultiplication": true,
            "RedundantArithmetic": true,
            "RedundantNot": true,
            "RedundantComparison": true,
            "MergeableEqual": true
          }
        }
      }
    }
  },
  "main": "./client/out/extension",
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -b",
    "watch": "tsc -b -w"
  },
  "devDependencies": {
    "@types/node": "^16.11.6",
    "@types/vscode": "^1.62.0",
    "@typescript-eslint/eslint-plugin": "^5.3.0",
    "@typescript-eslint/parser": "^5.3.0",
    "eslint": "^8.2.0",
    "typescript": "^4.4.4"
  },
  "dependencies": {
    "vscode-languageclient": "^7.0.0"
  }
}
