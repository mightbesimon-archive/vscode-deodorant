{
  "name": "json-extension",
  "description": "Simple json extension example",
  "author": "Open Law Library",
  "repository": "https://github.com/openlawlibrary/pygls",
  "license": "Apache-2.0",
  "version": "0.11.3",
  "publisher": "openlawlibrary",
  "engines": {
    "vscode": "^1.62.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:python"
  ],
  "contributes": {
    "commands": [],
    "configuration": {
      "type": "object",
      "title": "pyDeodoriser",
      "properties": {
        "pyDeodoriser.substructures": {
          "type": "object",
          "description": "Toggle the detection of different patterns.",
          "properties": {
            "UnnecessaryElif": {
              "type": "boolean"
            },
            "IfElseReturnBool": {
              "type": "boolean"
            },
            "IfReturnBool": {
              "type": "boolean"
            }
          },
          "additionalProperties": false,
          "default": {
            "UnnecessaryElif": true,
            "IfElseReturnBool": true,
            "IfReturnBool": true
          }
        }
      }
    }
  },
  "main": "./client/out/extension",
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -b",
    "watch": "tsc -b -w"
  },
  "devDependencies": {
    "@types/node": "^16.11.6",
    "@types/vscode": "^1.62.0",
    "@typescript-eslint/eslint-plugin": "^5.3.0",
    "@typescript-eslint/parser": "^5.3.0",
    "eslint": "^8.2.0",
    "typescript": "^4.4.4"
  },
  "dependencies": {
    "vscode-languageclient": "^7.0.0"
  }
}
